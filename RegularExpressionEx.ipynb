   "outputs": [],
   "source": [
    "import re"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 9,
   "metadata": {},
   "outputs": [],
   "source": [
    "text = \"문의사항은 032-123-3915 로 연락주시기 바랍니다.\"\n",
    "pattern = re.compile(r'\\d\\d\\d-\\d\\d\\d-\\d\\d\\d\\d') # \\d : 숫자 0~9"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 8,
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "032-123-391\n"
     ]
    }
   ],
   "source": [
    "search = pattern.search(text)\n",
    "if search:\n",
    "    telNumber = search.group()\n",
    "    print(telNumber)\n",
    "else:\n",
    "    print(\"매칭 실패\")"
   ]
  }
